{
    "root": true,
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "html":true,
            "vue":true
        },
        "sourceType": "module"
    },
    "plugins":[
        "html",
        "vue"
    ],
    "rules": {
        "arrow-parens": 0,
        "generator-star-spacing": 0,
        "no-debugger": 0,
        "semi": ["error", "always"],
        "indent": 0,
        "eol-last": 0,
        "space-before-function-paren": 0,
        "one-var": 0,
        "eqeqeq": 0
    }
}